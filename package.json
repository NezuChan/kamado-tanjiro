{
  "version": "1.0.0",
  "name": "kamado-tanjiro",
  "description": "A discord moderation bot.",
  "dependencies": {
    "@discordjs/builders": "0.8.2",
    "@sapphire/decorators": "3.1.4",
    "@sapphire/framework": "2.1.3",
    "@sapphire/plugin-subcommands": "2.0.1",
    "@sapphire/utilities": "3.0.7",
    "chalk": "4.1.2",
    "common-tags": "1.8.0",
