{
  "version": "1.0.0",
  "name": "kamado-tanjiro",
  "description": "A discord moderation bot.",
  "dependencies": {
    "@discordjs/builders": "0.10.0",
    "@sapphire/decorators": "3.1.4",
    "@sapphire/framework": "2.2.2",
    "@sapphire/plugin-api": "3.1.2",
    "@sapphire/plugin-scheduled-tasks": "1.0.0",
    "@sapphire/plugin-subcommands": "2.1.3",
    "@sapphire/utilities": "3.1.0",
    "bull": "4.2.0",
    "chalk": "4.1.2",
    "common-tags": "1.8.2",
    "discord.js": "13.4.0",
    "dotenv": "10.0.0",
    "enmap": "5.8.7",
    "mongodb": "3.7.3",
    "petitio": "1.4.0",
    "typeorm": "0.2.41"
  },
  "scripts": {
    "start": "ts-node -r dotenv/config src",
    "compile": "rimraf dist && tsc",
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "tests": "npm run compile && npm run lint",
    "tests:fix": "npm run compile && npm run lint:fix"
  },
  "engines": {
    "node": ">=v16.x"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "7.0.1",
    "@types/bull": "3.15.7",
    "@types/common-tags": "1.8.1",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.17",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "8.5.0",
    "eslint-plugin-import": "2.25.3",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "sort-keys": 0,
      "@typescript-eslint/no-non-null-asserted-optional-chain": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/no-unnecessary-condition": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/no-extraneous-class": 0,
      "@typescript-eslint/member-ordering": 0,
      "@typescript-eslint/restrict-template-expressions": 0,
      "no-eq-null": 0,
      "no-negated-condition": 0,
      "@typescript-eslint/prefer-optional-chain": 0,
      "@typescript-eslint/no-unsafe-argument": 0,
      "@typescript-eslint/naming-convention": 0,
      "@typescript-eslint/no-base-to-string": 0
    },
    "ignorePatterns": []
  }
}
